generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model TodoItem {
  id        String   @id @default(cuid())
  complete  Boolean  @default(false)
  title     String 
  comment   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}